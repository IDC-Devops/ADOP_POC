[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/IDC-Devops
[access "refs/for/refs/*"]
	read = group user/IDC-Devops
	push = group user/IDC-Devops
	label-Code-Review = -2..+2 group user/IDC-Devops
	label-Verified = -1..+1 group user/IDC-Devops
	rebase = group user/IDC-Devops
	removeReviewer = group user/IDC-Devops
	submit = group user/IDC-Devops
[access "refs/heads/*"]
	read = group user/IDC-Devops
	create = group user/IDC-Devops
	push = +force group user/IDC-Devops
	pushMerge = group user/IDC-Devops
[access "refs/tags/*"]
	push = group user/IDC-Devops
	push = +force group user/IDC-Devops
